{
	"task-runners": [
		{
			"runner-type": "javascript",
			"workdir": "/home/node",
			"command": "/usr/local/bin/node",
			"args": [
				"--disallow-code-generation-from-strings",
				"--disable-proto=delete",
				"/usr/local/lib/node_modules/n8n/node_modules/@n8n/task-runner/dist/start.js"
			],
			"allowed-env": [
				"PATH",
				"GENERIC_TIMEZONE",
				"NEWFLOW_RUNNERS_GRANT_TOKEN",
				"NEWFLOW_RUNNERS_TASK_BROKER_URI",
				"NEWFLOW_RUNNERS_MAX_PAYLOAD",
				"NEWFLOW_RUNNERS_MAX_CONCURRENCY",
				"NEWFLOW_RUNNERS_TASK_TIMEOUT",
				"NEWFLOW_RUNNERS_HEARTBEAT_INTERVAL",
				"NEWFLOW_RUNNERS_HEALTH_CHECK_SERVER_ENABLED",
				"NEWFLOW_RUNNERS_HEALTH_CHECK_SERVER_HOST",
				"NEWFLOW_RUNNERS_HEALTH_CHECK_SERVER_PORT",
				"NODE_FUNCTION_ALLOW_BUILTIN",
				"NODE_FUNCTION_ALLOW_EXTERNAL",
				"NODE_OPTIONS",
				"NODE_PATH",
				"NEWFLOW_SENTRY_DSN",
				"NEWFLOW_VERSION",
				"ENVIRONMENT",
				"DEPLOYMENT_NAME"
			]
		}
	]
}
